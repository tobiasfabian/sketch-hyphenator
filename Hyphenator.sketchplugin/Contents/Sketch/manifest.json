{
  "compatibleVersion": 3,
  "bundleVersion": 1,
  "description": "Add soft hyphens to your text layers.",
  "icon": "icon.png",
  "commands": [
    {
      "name": "Add Hyphens",
      "icon": "add-hyphens.png",
      "description": "Add hyphens to selected layers.",
      "handler": "addHyphens",
      "identifier": "addHyphens",
      "script": "hyphenate.js"
    },
    {
      "name": "Remove Hyphens",
      "icon": "remove-hyphens.png",
      "description": "Remove hyphens from selected layers.",
      "handler": "removeHyphens",
      "identifier": "removeHyphens",
      "script": "hyphenate.js"
    },
    {
      "name": "Change Language",
      "handler": "changeLanguage",
      "description": "Change hypenator language for this document.",
      "identifier": "changeLanguage",
      "script": "hyphenate.js"
    }
  ],
  "menu": {
    "title": "Hyphenator",
    "items": [
      "addHyphens",
      "removeHyphens",
      "changeLanguage"
    ]
  },
  "version": "0.2.0",
  "homepage": "https://github.com/tobiasfabian/Sketch-Hyphenator",
  "name": "Hyphenator",
  "disableCocoaScriptPreprocessor": true,
  "appcast": "https://raw.githubusercontent.com/tobiasfabian/sketch-hyphenator/master/.appcast.xml",
  "author": "Tobias Wolf",
  "authorEmail": "hello@tobiaswolf.me"
}